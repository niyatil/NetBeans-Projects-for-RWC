
/**
 *
 * @author Niyati
 */
public class LogicalOperator {
    public static void main(String []args){
        int a = 1, b = 2, c = 9;
        boolean result;

        result = (b < a)||(c > b);
        System.out.println(result);

        result = (b < a)&&(c > b);
        System.out.println(result);
    }
}
 
